      }],
      "expected_output": "Кількість зупинок: 2, кількість пересадок: 0"
    },
    {
      "function_name": "calculate_stops_and_transfers",
      "input": ["Залізничний вокзал", "пл. Кропивницького", {
        "1": {
          "forward": ["Залізничний вокзал", "Приміський вокзал", "пл. Кропивницького"],
          "backward": ["пл. Кропивницького", "Приміський вокзал", "Залізничний вокзал"]
        },
        "4": {
          "forward": ["Залізничний вокзал", "вул. Коломийська"],
          "backward": ["вул. Коломийська", "Залізничний вокзал"]
        }
      }],
      "expected_output": "Кількість зупинок: 2, кількість пересадок: 0"
    },
    {
      "function_name": "calculate_stops_and_transfers",
